/*table*/
.table {
  border:2rpx solid #dadada;
  border-right: 0;
  border-bottom: 0;
  width: 100%;
  margin-bottom: 20rpx;
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th,.td {
  padding: 16rpx 10rpx;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width:100%;
  font-size: 30rpx;
  color: #666;
}
.th {
  font-weight: bold;
  background-color: #dadada;
}
.textL{
  text-align: left;
}
.textC{
  text-align: center;
}
.noborder{
  border: none;
}
.noborder .th,.noborder .td{
  border-right: none;
}
.noindent{
  text-indent: 0;
}
.nameBlack{
  color: #333;
  font-size: 34rpx;
  margin-right: 10rpx;
  font-weight: bold;
}
.nameGreen{
  color: #026a19;
  font-size: 34rpx;
  font-weight: bold;
}
.jobTitle{
  background: #dadada;
  font-weight: bold;
  display: block;
  font-size: 30rpx;
  height:70rpx;
  line-height: 70rpx;
  padding-left: 10rpx;
  margin-bottom: 10rpx;
  color: #666;
}
.table .td navigator{
  text-decoration: underline;
  color: #026a19;
}
.contactInfo{
  color:#555;
  line-height: 48rpx;
  font-size: 32rpx;
}
 
