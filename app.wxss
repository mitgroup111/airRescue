/**app.wxss**/
@import "/pages/wxParse/wxParse.wxss";
@import "icon.wxss";
.fl{
	float:left;
}
.fr{
	float:right;
  
}
.clear{
	clear:both;
}
.clearfix:after { 
	content:"\200B"; 
	display:block;
	height:0; 
	clear:both; 
} 
input[type='text'],
input[type='number'],
input[type='password'],
input[type='reset'],
input[type='submit'],
input[type='button'],
input[type='tel'],
button,
textarea{
	-webkit-appearance:none; 
	border-radius:6rpx; 
	border:2prx solid #e0e0e0;
	font-family:Microsoft YaHei;
	font-size:32rpx;
}
page{  
  height: 100%;  
}   
.m-t-10{
  margin-top: 10rpx;
}
.m-t-20{
  margin-top: 20rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:0;
  box-sizing: border-box;
  background-color: #f2f2f2 
} 
.marginBottom{
  margin-bottom: 20rpx;
}
/*图片切换banner*/
.swiper-container{
 position: relative;
}
.swiper-container .swiper{
 height: 320rpx;
}
.swiper-container .swiper .img{
 width: 100%;
 height: 100%;
}
.swiper-container .dots{
 position: absolute;
 left: 0;
 right: 0;
 bottom: 20rpx;
 display: flex;
 justify-content: center;
}
.swiper-container .dots .dot{
 margin: 0 8rpx;
 width: 14rpx;
 height: 14rpx;
 background: #fff;
 border-radius: 8rpx;
 transition: all .6s;
}
.swiper-container .dots .dot.active{
 width:14rpx;
 background: #f80;
}
.contentW{
  width: 95%;
  margin: 0 auto;
  margin-bottom: 20rpx;
  border-top:2rpx solid #ddd;
  padding-top:20rpx;
}
.aboutTitle{
    margin: 20rpx 0 30rpx 0;
    height:64rpx;
}
.aboutTitleSpan{
    border-bottom: 6rpx solid #026a19;
    color: #026a19;
    font-size: 38rpx;
}
.aboutH{
  font-size: 32rpx;
  display: block;
  margin-bottom: 10rpx;
  font-weight: bold;
  color:#555;
}
.aboutInfo{
    font-size: 34rpx;
    line-height: 60rpx;
    margin-bottom: 10rpx;
    text-indent: 52rpx;
    display: block;
    color:#666;
}
.aboutInfoSpan{
  color:#c20c0c;
