.order-menu-list{
  color: #555;
  font-size: 35rpx;
  position: relative;
}
.order-menu-list-name{
 line-height: 80rpx;
 border-top: 2rpx solid #ddd;
 color: #888;
}
.order-menu-list:before{
  content: '';
  display: block;
  height: 2rpx;
  background-color: #c8c7cc;
  position: absolute;
  bottom: 0;
  left: 20rpx;
}
/*下箭头*/
.xiajiantou:after{
  font-family:"iconfont" !important;
  content: '\e733';
  font-size: 40rpx;
  position: absolute;
  top:30rpx;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  right: 10rpx;
}
/*上箭头*/
.shangjiantou:after{
  font-family:"iconfont" !important;
  content: '\e734';
  font-size: 40rpx;
  position: absolute;
  top: 30rpx;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  right: 10rpx;
}
/*右箭头*/
.youjiantou:after{
  font-family:"iconfont" !important;
  content: '\e735';
  font-size: 40rpx;
  position: absolute;
  top: 50%;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  right: 10rpx;
}
